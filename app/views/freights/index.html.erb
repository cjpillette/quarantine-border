	<nav class="navbar navbar-default">
		<%= button_to 'Enter new freight', new_freight_path, method: :get, class: 'btn btn-default' %>
		<%= button_to 'Back', root_path, class: 'btn btn-default' %>
	</nav>




<div class="wrapper">
	<table class="table table-striped table-bordered">
		<tr>
			<th>Airport</th>
			<th>Date</th>
			<th>Carrier / flight</th>
			<th>Freight type</th>			
		</tr>
				<% @freights.each do |freight| %>
					<tr>
						<th><%= freight.airport.name%></th>
						<th><%= freight.inspectedon %></th>
						<th><%= freight.carrier %></th>
						<th><%= link_to freight.brmtype.name, edit_freight_path(freight) %></th>					
					</tr>
				<% end %>  
		
	</table>
</div>


